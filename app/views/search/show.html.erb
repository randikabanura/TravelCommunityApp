<div class="container">
<h1>Search results for <%=@search_term %></h1>
  <% if !@users.empty? %>
    <% @users.each do |user|%>
      <h2><%= link_to user.name, profile_show_path(user) %></h2>
    <% end %>
  <% end %>

   <% if !@trips.empty? %>
    <% @trips.each do |trip|%>
      <h2><%= link_to trip.location, trip_path(trip) %></h2>
    <% end %>
  <% end %>
</div>